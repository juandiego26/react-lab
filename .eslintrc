{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "function-paren-newline": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "jsx-a11y/anchor-is-valid": [ "error", {
            "components": [ "Link" ],
            "specialLink": [ "hrefLeft", "hrefRight","to" ],
            "aspects": [ "noHref", "invalidHref", "preferButton" ]
          }]
    },
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    },
}
